#define fontface Input
#define fontsize 14

URxvt*scrollBar: false

URxvt*font: xft:fontface :pixelsize=fontsize
URxvt*italicFont: xft:fontface :italic :pixelsize=fontsize
URxvt*boldFont: xft:fontface :bold :pixelsize=fontsize

URxvt*depth: 32
URxvt*foreground: base0F
URxvt*background: [85]base00
URxvt*cursorColor: base0F
