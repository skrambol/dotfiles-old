#define fontface Input
#define fontsize 12

URxvt*scrollBar: false

URxvt*font: xft:fontface :pixelsize=fontsize:antialias=true
URxvt*italicFont: xft:fontface :pixelsize=fontsize:Italic
URxvt*boldFont: xft:fontface :pixelsize=fontsize:Bold
URxvt*boldItalicFont: xft:fontface :pixelsize=fontsize:BoldItalic

URxvt*depth: 32
URxvt*foreground: base0F
URxvt*background: [85]base00
URxvt*cursorColor: base0F
